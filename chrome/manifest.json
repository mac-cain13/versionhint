{
  "name": "VersionHint",
  "description": "Tells you what version they're talking about.",
  "version": "1.0.0",
  "author": "Mathijs Kadijk",

  "manifest_version": 2,
  "minimum_chrome_version": "37",
  "permissions": [],
  "icons": {
    "128": "images/icon-128.png",
    "48": "images/icon-48.png",
    "16": "images/icon-16.png"
  },

  "content_scripts": [ {
    "js": ["jquery.js", "underscore.js", "platformData.js", "content.js" ],
    "matches": [ "*://*.stackoverflow.com/*" ]
  } ]

}
